    "@iconify-json/carbon": "^1.1.35",
    "@types/node": "^20.14.4",
    "@unocss/eslint-config": "^0.61.0",
    "@unocss/eslint-plugin": "^0.61.0",
    "@unocss/reset": "^0.61.0",
    "@uxuip/eslint-config": "^1.0.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue-macros/volar": "^0.22.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
